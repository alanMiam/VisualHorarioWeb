<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlets.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletValidarPass</servlet-name>
        <servlet-class>servlets.ServletValidarPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletRegistro</servlet-name>
        <servlet-class>servlets.ServletRegistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEnviarReg</servlet-name>
        <servlet-class>servlets.ServletEnviarReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletActUs</servlet-name>
        <servlet-class>servlets.ServletActUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEliminacionUsuario</servlet-name>
        <servlet-class>servlets.ServletEliminacionUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletActualizacionUsuario</servlet-name>
        <servlet-class>servlets.ServletActualizacionUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCrearClase</servlet-name>
        <servlet-class>servlets.ServletCrearClase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletRegistrarClase</servlet-name>
        <servlet-class>servlets.ServletRegistrarClase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletElimClase</servlet-name>
        <servlet-class>servlets.ServletElimClase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEliminacionClase</servlet-name>
        <servlet-class>servlets.ServletEliminacionClase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletHorario</servlet-name>
        <servlet-class>servlets.ServletHorario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletValidarPass</servlet-name>
        <url-pattern>/validarpass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRegistro</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEnviarReg</servlet-name>
        <url-pattern>/enviar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletActUs</servlet-name>
        <url-pattern>/actualizarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEliminacionUsuario</servlet-name>
        <url-pattern>/eliminacionUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletActualizacionUsuario</servlet-name>
        <url-pattern>/actualizacionUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCrearClase</servlet-name>
        <url-pattern>/crearClase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRegistrarClase</servlet-name>
        <url-pattern>/registrarClase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletElimClase</servlet-name>
        <url-pattern>/eliminarClase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEliminacionClase</servlet-name>
        <url-pattern>/eliminacionClase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletHorario</servlet-name>
        <url-pattern>/horario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
